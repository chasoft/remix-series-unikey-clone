{
  "private": true,
  "name": "remix-app-template-js",
  "description": "",
  "license": "",
  "dependencies": {
    "@remix-run/cloudflare-pages": "^1.2.1",
    "@remix-run/react": "^1.2.1",
    "esbuild": "^0.14.22",
    "front-matter": "^4.0.2",
    "marked": "^4.0.12",
    "nprogress": "^0.2.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@remix-run/dev": "^1.2.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "scripts": {
    "postinstall": "remix setup cloudflare-pages",
    "build": "cross-env NODE_ENV=production npm run buildCss && remix build",
    "buildCss": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run devCss\" \"remix build && run-p dev:*\"",
    "devCss": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler"
  }
}